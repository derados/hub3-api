{
    "name": "bigfish/hub3-api",
    "description": "An API for creating 2D barcodes following the Croatian Banking Association's HUB3 standard",
    "require": {
        "php": ">=5.5",
        "ext-bcmath": "*",
        "bigfish/pdf417": "~0.1.2",
        "erusev/parsedown": "^1.6.0",
        "justinrainbow/json-schema": "^3.0.0",
        "silex/silex": "~1.2",
        "symfony/twig-bridge": "~2.5",
        "twig/twig": "~1.0"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*"
    },
    "autoload": {
        "psr-4": {
            "BigFish\\Hub3\\Api\\": "src/"
        }
    }
}
